{
    "name":         "jms/job-queue-bundle",
    "description":  "Allows to run and schedule Symfony console commands as background jobs.",
    "keywords":     ["job","queue", "background", "cron", "scheduled"],
    "homepage":     "http://jmsyst.com/bundles/JMSJobQueueBundle",
    "type":         "symfony-bundle",
    "license":      "Apache-2.0",
    "authors": [
        {
            "name":   "Johannes M. Schmitt",
            "email":  "schmittjoh@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-pcntl": "*",
        "symfony/framework-bundle": "^6.0",
        "symfony/error-handler": "^6.0",
        "doctrine/common": "^3.4",
        "symfony/process": "^6.0",
        "doctrine/annotations": "^2.0",
        "symfony/cache": "^6.0",
        "symfony/console": "^6.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "*",
        "sensio/framework-extra-bundle": "*",
        "symfony/class-loader": "*",
        "symfony/yaml": "*",
        "symfony/browser-kit": "*",
        "symfony/finder": "*",
        "symfony/css-selector": "*",
        "doctrine/doctrine-bundle": "*",
        "doctrine/orm": "*",
        "symfony/twig-bundle": "*",
        "symfony/form": "*",
        "symfony/validator": "*",
        "phpunit/phpunit": "^10.0",
        "symfony/intl": "*"
    },
    "suggest": {
        "sensio/framework-extra-bundle": "Required when using the webinterface.",
        "symfony/twig-bundle": "Required when using the webinterface."
    },
    "autoload": {
        "psr-4": { "JMS\\JobQueueBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "JMS\\JobQueueBundle\\Tests\\": "Tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
